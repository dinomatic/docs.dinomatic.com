{
  "plugins": ["prettier-plugin-tailwindcss", "prettier-plugin-jinja-template"],
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "semi": false,
  "bracketSpacing": true,
  "bracketSameLine": true,
  "singleAttributePerLine": false,
  "printWidth": 120,
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "parser": "jinja-template",
        "tabWidth": 4
      }
    }
  ]
}
